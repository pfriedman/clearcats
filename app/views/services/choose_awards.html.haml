- stylesheet "services"
- javascript "jquery.blockUI", "modal_form"
- title "New Service - Choose Awards"

:javascript
  $(function () { $(".datepicker").live('mouseover', function() { $(this).datepicker(); }); });

= render "menu"

%p
  = @service.person.employeeid
  
= render '/awards/list', :service => @service, :awards => @awards, :person => @service.person, :search_params => @search_params

- form_for @service do |f|
  = hidden_field_tag "service[state]", "choose_publications"
  .buttons
    = f.submit "Continue", :disable_with => 'Continuing...'


#modal_window